# Copy this file to .env.local and fill in the values. Do NOT commit secrets.

# Required for vendor OAuth start
ALICE_APP_CODE=your_alice_app_code_here

# Required for vendor callback / API requests
ALICE_API_SECRET=your_alice_api_secret_here

# Standard OAuth (if using OAuth token endpoint flow)
ALICE_CLIENT_ID=your_client_id_here
ALICE_CLIENT_SECRET=your_client_secret_here
# The redirect that the OAuth provider should call back to
# For local dev it is usually http://localhost:3000/api/alice/oauth/vendor/callback
ALICE_REDIRECT_URI=http://localhost:3000/api/alice/oauth/vendor/callback

# Optional behavior
ALICE_ALLOW_LOCAL_TOKEN_SAVE=false
ALICE_OAUTH_TOKEN_FILE=.alice.token
ALICE_OAUTH_TOKENS_FILE=.alice.tokens.json

# Other settings the app may use
# ALICE_APP_ORIGIN=http://localhost:3000
